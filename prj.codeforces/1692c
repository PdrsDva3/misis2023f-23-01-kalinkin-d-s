#include <iostream>
#include <vector>
#include <string>
//идея ясна, реализация хромает
//нужно вычислить, где касается слон правой стенки в двух местах или в двух левых местах
//вычислить y=kx+b и найти пересечение двух прямых
int main(){
    int t = 0;
    std::cin >> t;
    std::vector<std::string> desks;
    while (t > 0){
        --t;
        int matrix = 8;
        std::string temp;
        while (matrix > 0){
            --matrix;
            std::string tp;
            std::cin >> tp;
            temp+=tp;
            
        }
        desks.push_back(temp);
    }
    std::cout << "=================" << std::endl;
    for (std::string i : desks){
        std::cout << i << std::endl;
        std::cout << "=++===+++===++" << std::endl;
    }
}
